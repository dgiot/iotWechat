/**index.wxss**/

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #aaa;
}
.container{
  background-color: #166ead;
  height: 100vh;
}

.textView{
line-height: 50vh;
color: #fff;
font-size: 20px;
}

.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  z-index: 3;
}


.imgBg{
  position: absolute;
  top: 0px;
z-index: 1;
}
.imgIcon{
  position: absolute;
  top: 200px;
  width: 60%;
  height: 100px;
  z-index: 2;
}